#!/usr/bin/env bash
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
[ -f "/etc/node/init_node.sh" ] && . "/etc/node/init_node.sh"
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
[ -n "$NODE_MANAGER" ] || export NODE_MANAGER="fnm"
[ -n "$NODE_VERSION" ] || export NODE_VERSION="22"
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
# end
